class Solution(object):
    def isValid(self, s):
        stack=[]
        pairs={']':'[','}':'{',')':'('}
        for c in s:
            if c in "{([":
                stack.append(c)
            elif not stack or stack[-1] != pairs[c]:
                return False
            else:
                stack.pop()
        if not stack:
            return True
        else:
            return False
            
        
